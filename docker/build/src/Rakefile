# frozen-string-literal: true

require 'bundler/setup'

Rake.add_rakelib 'tasks/shared'

case ENV['DB_TYPE'].to_sym
when :starrocks
  Rake.add_rakelib 'tasks/starrocks'
when :mssql
  Rake.add_rakelib 'tasks/mssql'
else
  raise ArgumentError, 'Db type net yet supported, or you need to specify you DB_TYPE'
end
